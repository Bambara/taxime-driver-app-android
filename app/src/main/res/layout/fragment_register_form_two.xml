<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/root_layout_register_form_two_fragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.snap.register.RegisterFormTwoFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <EditText
            android:id="@+id/city"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/shape_register_edit_text"
            android:ems="10"
            android:hint="@string/city"
            android:inputType="textPostalAddress"
            android:paddingLeft="16dp"
            android:textAlignment="textStart"
            android:textColor="@color/black"
            android:textColorHint="@color/ashThree"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/district"
            tools:ignore="DuplicateSpeakableTextCheck" />

        <EditText
            android:id="@+id/accNumber"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/shape_register_edit_text"
            android:ems="10"
            android:hint="@string/accNumber"
            android:inputType="number"
            android:paddingLeft="16dp"
            android:textAlignment="textStart"
            android:textColor="@color/black"
            android:textColorHint="@color/ashThree"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/city" />

        <EditText
            android:id="@+id/district"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/shape_register_edit_text"
            android:ems="10"
            android:hint="@string/district"
            android:inputType="textPostalAddress"
            android:paddingLeft="16dp"
            android:textAlignment="textStart"
            android:textColor="@color/black"
            android:textColorHint="@color/ashThree"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/province" />

        <EditText
            android:id="@+id/province"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:background="@drawable/shape_register_edit_text"
            android:ems="10"
            android:hint="@string/province"
            android:inputType="text"
            android:paddingLeft="16dp"
            android:textAlignment="textStart"
            android:textColor="@color/black"
            android:textColorHint="@color/ashThree"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/city"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/city"
            app:layout_constraintTop_toBottomOf="@+id/country" />

        <EditText
            android:id="@+id/country"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/shape_register_edit_text"
            android:ems="10"
            android:hint="@string/country"
            android:inputType="textPostalAddress"
            android:paddingLeft="16dp"
            android:textAlignment="textStart"
            android:textColor="@color/black"
            android:textColorHint="@color/ashThree"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/country_error"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginStart="2dp"
            app:layout_constraintBottom_toBottomOf="@+id/country"
            app:layout_constraintStart_toEndOf="@+id/country" />

        <ImageView
            android:id="@+id/province_error"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginStart="2dp"
            app:layout_constraintBottom_toBottomOf="@+id/province"
            app:layout_constraintStart_toEndOf="@+id/province" />

        <ImageView
            android:id="@+id/district_error"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginStart="2dp"
            android:layout_marginBottom="7dp"
            app:layout_constraintBottom_toBottomOf="@+id/district"
            app:layout_constraintStart_toEndOf="@+id/district" />

        <ImageView
            android:id="@+id/city_error"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginStart="2dp"
            app:layout_constraintBottom_toBottomOf="@+id/city"
            app:layout_constraintStart_toEndOf="@+id/city" />

        <ImageView
            android:id="@+id/account_error"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginStart="2dp"
            app:layout_constraintBottom_toBottomOf="@+id/accNumber"
            app:layout_constraintStart_toEndOf="@+id/accNumber" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>
